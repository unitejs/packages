{
    "bluebird": {
        "name": "bluebird",
        "main": "/js/browser/bluebird.js",
        "mainMinified": "/js/browser/bluebird.min.js",
        "includeMode": "both",
        "scriptIncludeMode": "both"
    },
    "bootstrap4": {
        "name": "bootstrap",
        "version": "4.0.0-beta",
        "includeMode": "both",
        "noScript": true
    },
    "font-awesome": {
        "name": "font-awesome",
        "includeMode": "both",
        "assets": [
            "fonts/**/*"
        ]
    },
    "prismjs": {
        "name": "primsjs",
        "main": "prism.js",
        "includeMode": "both",
        "assets": [
            "components/**/*.min.js"
        ]
    },
    "moment": {
        "name": "moment",
        "main": "moment.js",
        "mainMinified": "min/moment.min.js",
        "includeMode": "both"
    },
    "rxjs": {
        "name": "rxjs",
        "main": "*",
        "mainLib": [
            "*.js",
            "add/**/*.js",
            "observable/**/*.js",
            "operator/**/*.js",
            "operators/**/*.js",
            "scheduler/**/*.js",
            "symbol/**/*.js",
            "testing/**/*.js",
            "util/**/*.js"
        ],
        "includeMode": "both"
    }
}