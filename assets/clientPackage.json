{
    "bluebird": {
        "name": "bluebird",
        "main": "/js/browser/bluebird.js",
        "mainMinified": "/js/browser/bluebird.min.js",
        "scriptIncludeMode": "both"
    },
    "bootstrap4": {
        "name": "bootstrap",
        "noScript": true
    },
    "font-awesome": {
        "name": "font-awesome",
        "assets": [
            "fonts/**/*"
        ]
    },
    "lodash": {
        "name": "lodash",
        "main": "lodash.js",
        "mainMinified": "lodash.min.js"
    },
    "@types/lodash": {
        "name": "@types/lodash",
        "isDevDependency": true
    },
    "prismjs": {
        "name": "primsjs",
        "main": "prism.js",
        "assets": [
            "components/**/*.min.js"
        ]
    },
    "moment": {
        "name": "moment",
        "main": "moment.js",
        "mainMinified": "min/moment.min.js"
    },
    "rxjs": {
        "name": "rxjs",
        "main": "*",
        "libFile": "index.js",
        "libExtension": "js",
        "mainLib": [
            "operator",
            "operators",
            "observable",
            "scheduler",
            "symbol",
            "testing",
            "add/operator",
            "add/observable",
            "internal",
            "internal/observable",
            "internal/operators",
            "internal/scheduler",
            "internal/symbol",
            "internal/testing",
            "internal/util",
            "util"
        ]
    },
    "rxjs-compat": {
        "name": "rxjs-compat",
        "main": "*",
        "mainLib": [
            "operator",
            "operators",
            "observable",
            "scheduler",
            "symbol",
            "testing",
            "add/operator",
            "add/observable",
            "internal",
            "internal/observable",
            "internal/operators",
            "internal/scheduler",
            "internal/symbol",
            "internal/testing",
            "internal/util",
            "util"
        ]
    },
    "underscore": {
        "name": "underscore",
        "main": "underscore.js",
        "mainMinified": "underscore-min.js"
    },
    "@types/underscore": {
        "name": "@types/underscore",
        "isDevDependency": true
    }
}
